                                                        t-att-data-template_type="qc_line['template_type']"
                                                        t-att-data-sequence="qc_line['sequence']"
                                                        class="text-right eq-input"/>
                                            </t>
                                            <t t-if="qc_line['template_type'] == 'employee'">
                                                <input type="text" id="input_employee_none"
                                                        t-att-data-equiptment_id="qc_line['equipment_id']"
                                                        t-att-data-qc_line_id="qc_line['qc_line_id']"
                                                        t-att-data-time="line['hour']"
                                                        t-att-value="qc_line['value']"
                                                        t-att-data-product="qc_line['product_id']"
                                                        t-att-data-location="qc_line['location_id']"
                                                        t-att-data-qc_type="qc_line['qc_type']"
                                                        t-att-data-qc_line_type="qc_line['qc_line_type']"
                                                        t-att-data-template_type="qc_line['template_type']"
                                                        t-att-data-sequence="qc_line['sequence']"
                                                        class="text-right eq-input"/>
                                            </t>
                                            <t t-if="qc_line['template_type'] == 'lot'">
                                                <input type="text" id="input_lot_none"
                                                        t-att-data-equiptment_id="qc_line['equipment_id']"
                                                        t-att-data-qc_line_id="qc_line['qc_line_id']"
                                                        t-att-data-time="line['hour']"
                                                        t-att-value="qc_line['value']"
                                                        t-att-data-product="qc_line['product_id']"
                                                        t-att-data-location="qc_line['location_id']"
                                                        t-att-data-qc_type="qc_line['qc_type']"
                                                        t-att-data-qc_line_type="qc_line['qc_line_type']"
                                                        t-att-data-template_type="qc_line['template_type']"
                                                        t-att-data-sequence="qc_line['sequence']"
                                                        class="text-right eq-input"/>
                                            </t>
                                            <t t-if="qc_line['template_type'] == 'time'">
                                                <input type="time" id="input_time_none"
                                                        t-att-data-equiptment_id="qc_line['equipment_id']"
                                                        t-att-data-qc_line_id="qc_line['qc_line_id']"
                                                        t-att-data-time="line['hour']"
                                                        t-att-value="qc_line['value']"
                                                        t-att-data-product="qc_line['product_id']"
                                                        t-att-data-location="qc_line['location_id']"
                                                        t-att-data-qc_type="qc_line['qc_type']"
                                                        t-att-data-qc_line_type="qc_line['qc_line_type']"
                                                        t-att-data-template_type="qc_line['template_type']"
                                                        t-att-data-sequence="qc_line['sequence']"
                                                        class="text-right eq-input"/>
                                            </t>
                                        </t>
                                    </td>
                                </t>
                            </tr>
                            <t t-set="sequence" t-value="sequence+1"/>
                        </t>
                    </table>
					<t t-if="template" t-foreach="template['line']" t-as="tmp">
                     <t t-if="tmp['template_type'] == 'footer_text'">
                       <div class="row">
                        <p style="text-align:left; margin-left:30px;" class="col-sm-3">
                            <t t-att-data-id="tmp['id']" t-esc="tmp['name']"/>
                        </p>
						<div class="col-sm-2">
                          <t t-set="sequence" t-value="0"/>
                          <t t-foreach="lines" t-as="line">
                            <t t-foreach="line['qc_lines']" t-as="qc_line">
                              <t t-if="qc_line['template_type'] == 'footer_text'">
                                <t t-if="sequence == 0">
                                  <input type="text"
									t-attf-id="input-{{sequence}}-{{tmp['id']}}"
                                    t-att-data-equiptment_id="qc_line['equipment_id']"
                                    t-att-data-qc_line_id="tmp['id']"
                                    t-att-data-time="line['hour']"
                                    t-att-value="qc_line['value']"
                                    t-att-data-product="qc_line['product_id']"
                                    t-att-data-location="qc_line['location_id']"
                                    t-att-data-qc_type="qc_line['qc_type']"
                                    t-att-data-qc_line_type="qc_line['qc_line_type']"
                                    t-att-data-template_type="qc_line['template_type']"
                                    t-att-data-sequence="sequence"
                                    class="text-right eq-input"/>
                                </t>
                                <t t-set="sequence" t-value="sequence+1"/>
                              </t>
                            </t>
                           </t> 
                        </div>
                        </div>
                      </t>
                    </t>
                    <div class="row mt32 mb32">
                        <div class="col-xs-6" align="center">
                            <div class="col-xs-6">
                                <span style="color:#B5B4B4; font-size:13px;">INSPECTOR 1</span>
                                <button data-state="approve1" class="btn btn-lg btn-block btn-primary btn-approve" type="button">Approve 1</button>
                            </div>
                            <div class="col-xs-6">
                                <span style="color:#B5B4B4; font-size:13px;">INSPECTOR 2</span>
                                <button data-state="approve2" class="btn btn-lg btn-block btn-primary btn-approve" type="button">Approve 2</button>
                            </div>
                        </div>
                        <div class="col-xs-6" align="center">
                            <div class="alert alert-warning">
                                <button class="btn btn-lg btn-success btn-validate"
                                    t-attf-data-qc-group-id="{{current_qc_group_id}}"
                                    t-attf-data-qc-type="{{qc_type_code}}" type="button">VALIDATE</button>
                                <strong>Not be changed !</strong>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </template>
</odoo>
