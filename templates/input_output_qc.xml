<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="input_output_qc">
        <div class="o_form_view">
            <div class="page o_form_sheet_bg">
                <div class="o_form_sheet">
                    <div class="row">
                        <h1><strong style="padding-left:10px;"><span t-esc="qc_type"/> Data</strong></h1>
                        <h4 style="padding-left:10px;"><strong>Date : <span t-esc="date"/></strong></h4>
                        <h4 style="padding-left:10px;"><strong>On Workcenter : <span t-esc="workcenter_id"/></strong></h4>
                        <div class="panel panel-default">
                            <div class="panel-body">
                                <h3><strong style="padding-left:10px;">Equipment</strong></h3>
                                <div class="form-check form-check-inline">
                                    <t t-if="all_equipment">
                                        <t t-foreach="all_equipment" t-as="eq">
                                            <t t-if="equipment_selected[eq.get('equipment_id').id]">
                                                <input class="ac_ktis_qc_check_eq" type="radio" t-att-data-id="eq.get('equipment_id').id" t-attf-checked="checked"></input>
                                                <label class="form-check-label" for="inlineCheckbox1"><h3><b class="select-title"><t t-esc="eq.get('equipment_id').name"/></b></h3></label>
                                            </t>
                                            <t t-else="">
                                                <input class="ac_ktis_qc_check_eq" type="radio" t-att-data-id="eq.get('equipment_id').id"></input>
                                                <label class="form-check-label" for="inlineCheckbox1"><h4><t t-esc="eq.get('equipment_id').name"/></h4></label>
                                            </t>
                                        </t>
                                    </t>
                                </div>
                            </div>
                        </div>
                        <div class="panel panel-default">
                            <h3><strong style="padding-left:10px;">Templates</strong></h3>
                            <div class="panel-body">
                                <div class="form-check form-check-inline">
                                    <t t-foreach="header" t-as="head">
                                        <t t-if="template_selected[head['id']]">
                                            <span style="padding-left:10px; padding-right:10px;" class="col-sm-6">
                                                <input class="template_qc_check col-sm-1" type="radio" t-att-data-id="head['id']" t-attf-checked="checked"></input>
                                                <label class="form-check-label col-sm-11" for="inlineCheckbox1"><h4><t t-esc="head['name']"/></h4></label>
                                            </span>
                                        </t>
                                        <t t-else="">
                                            <span style="padding-left:10px; padding-right:10px;" class="col-sm-6">
                                                <input class="template_qc_check col-sm-1" type="radio" t-att-data-id="head['id']"></input>
                                                <label class="form-check-label col-sm-11" for="inlineCheckbox1"><h4><t t-esc="head['name']"/></h4></label>
                                            </span>
                                        </t>
                                    </t>
                                </div>
                            </div>
                        </div>
                    </div>
                    <table class="table table-condensed" name="table-qc">
                        <tr>
                            <td style="border:None"></td>
                            <t t-if="template" t-foreach="template['line']" t-as="tmp">
							  <t t-if="tmp['template_type'] != 'footer_text'">
                                <td t-attf-style="text-align:center" class="active">
                                   <t t-att-data-id="tmp['id']" t-esc="tmp['name']"/>
                                </td>
							  </t>
                            </t>
                        </tr>
                        <t t-foreach="lines" t-as="line">
                            <t t-set="sequence" t-value="0"/>
                            <tr>
                                <td>
                                    <t t-esc="line['hour']"/>
                                </td>
