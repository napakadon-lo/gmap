                            <h3><strong style="padding-left:10px;">Equipment</strong></h3>
                            <div class="panel-body">
                                <div class="form-check form-check-inline">
                                    <t t-foreach="header" t-as="head">
                                        <span style="padding-left:10px; padding-right:10px;">
                                            <t t-if="head['id'] == eq_selected">
                                                <div class="col-xs-4 col-md-4">
                                                    <input class="equipment_check" type="radio" t-att-data-id="head['id']" t-attf-checked="checked"></input>
                                                    <label class="form-check-label" for="inlineCheckbox1"><h3><b class="select-title"><t t-esc="head['name']"/></b></h3></label>
                                                </div>
                                            </t>
                                            <t t-else="">
